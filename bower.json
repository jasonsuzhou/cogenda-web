        },
        "bootstrap-switch": {
            "main": ["static/js/bootstrap-switch.js", "static/stylesheets/bootstrap-switch.css"]
        },
        "datatables-bootstrap3": {
            "main": ["BS3/assets/js/datatables.js", "BS3/assets/css/datatables.css"] 
        },
        "datatables": {
            "main": "media/js/jquery.dataTables.js"
        },
        "jquery-ui": {
            "main": "jquery-ui.min.js" 
        },
        "quicksearch": {
            "main": "dist/jquery.quicksearch.js"
        },
        "font-awesome": {
            "main": "css/font-awesome.min.css" 
        }
   }
